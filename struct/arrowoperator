#include<stdio.h>
struct sdata{
int regdno;
float mark;
char name[80];
};
int main(){
struct sdata s,*ptr;
ptr=&s;
scanf("%d\n",&ptr->regdno);//56
scanf("%f\n",&ptr->mark);//67.98
scanf("%s\n",&ptr->name);//Anindita
printf("%d\n",ptr->regdno);//print 56
printf("%f\n",ptr->mark);//print 67.98
printf("%s\n",ptr->name);//print Anindita

return 0;
}
